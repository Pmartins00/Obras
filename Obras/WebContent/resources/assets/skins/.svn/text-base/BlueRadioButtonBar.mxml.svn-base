<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:st="library://ns.tink.ws/flex/spark" alpha.disabled="0.5" xmlns:renderers="br.com.cestari.view.components.renderers.*">

	<fx:Metadata>
		<![CDATA[ 
		[HostComponent("spark.components.ButtonBar")]
		]]>
	</fx:Metadata>

	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>

	<fx:Declarations>
		<fx:Component id="middleButton">
			<renderers:RadioButtonItemRenderer buttonMode="true"/>
		</fx:Component>
	</fx:Declarations>

	<s:DataGroup id="dataGroup" height="100%" width="100%">
		<s:layout>
			<s:ButtonBarHorizontalLayout gap="3"/>
		</s:layout>
	</s:DataGroup>
</s:Skin>
