<!--- The default skin class for the Spark Button component.

	 @see spark.components.Button

	 @langversion 3.0
	 @playerversion Flash 10
	 @playerversion AIR 1.5
	 @productversion Flex 4
-->
<s:SparkSkin xmlns:mx="library://ns.adobe.com/flex/mx" minHeight="21" minWidth="21"
	xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009" alpha.disabled="0.5">
	<fx:Metadata>[HostComponent("br.com.cestari.components.popUpControls.PopUpButton")]</fx:Metadata>

	<fx:Script>
		<![CDATA[
			import br.com.cestari.assets.Assets;
		]]>
	</fx:Script>
	<!-- host component -->
	<!-- states -->
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>

	<mx:Image id="firePopUpElement" source="{Assets.ICON_POPUP_EDIT}"
		buttonMode="true" useHandCursor="true"/>

</s:SparkSkin>
